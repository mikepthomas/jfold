<document outputName="${project.name}"
          xmlns="http://maven.apache.org/DOCUMENT/1.0.1"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/DOCUMENT/1.0.1 http://maven.apache.org/xsd/document-1.0.1.xsd">
    <meta>
        <title>${project.name}</title>
        <author>${project.developers[0].name}</author>
    </meta>

    <toc name="Table of Contents">
        <item name="Introduction" ref="index.apt"/>
    </toc>

    <cover>
        <coverTitle>${project.name}</coverTitle>
        <coverSubTitle>${project.version}</coverSubTitle>
        <coverType>Project Documentation</coverType>

        <projectName>${project.name}</projectName>

        <companyName>${project.developers[0].name}</companyName>
        <companyLogo>http://www.stanford.edu/group/pandegroup/title4.jpg</companyLogo>
    </cover>
</document>
